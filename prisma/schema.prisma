generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model post {
  id            String  @id
  author        String?
  project_name  String?
  release_date  String?
  tech_stack    String?
  content       String?
  image         String?
  project_title String? @unique
}
